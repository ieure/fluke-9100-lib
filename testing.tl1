! General UUT utility program
!
! Author: Ian Eure <ian@retrospec.tv>
!
! Indicate that part(s) are being tested.
!

program testing(refs)
    declare
        string refs
        global numeric uutwin
    end declare

    define ref refs, mode "testing"
    if uutwin <> 0 then draw ref refs, channel uutwin
end program

